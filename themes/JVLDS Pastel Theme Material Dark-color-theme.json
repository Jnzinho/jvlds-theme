{
  "tokenColors": [
    {
      "settings": {
        "background": "#282433",
        "foreground": "#e8d7f5"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8b8198"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e8d7f5"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#c4b7e8"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "#c4b7e8"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#e8d7f5"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#c4b7e8"
      }
    },
    {
      "name": "JSX/TSX Props",
      "scope": [
        "meta.tag.attributes variable.other.readwrite",
        "meta.tag.attributes variable.other.property"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "JSX/TSX Component Props",
      "scope": [
        "meta.tag.attributes meta.embedded.expression variable.other.readwrite",
        "meta.tag.attributes meta.embedded.expression variable.other.object"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a8d7cd"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f7b3c3"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c4b7e8"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e8d7f5"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#f7b3c3",
        "fontStyle": "",
        "foreground": "#282433"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#c4b7e8",
        "foreground": "#282433"
      }
    },
    {
      "name": "Punctuation, Operators",
      "scope": [
        "punctuation",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#8b8198"
      }
    }
  ],
  "colors": {
    "editor.background": "#282433",
    "editorCursor.foreground": "#e8d7f5",
    "editor.foreground": "#e8d7f5",
    "editorWhitespace.foreground": "#3d3647",
    "editor.lineHighlightBackground": "#332e3f",
    "editor.selectionBackground": "#3d3647",
    "activityBar.background": "#282433",
