{
  "name": "jvlds-theme",
  "displayName": "Devine Theme",
  "description": "A delightful VS Code theme featuring two variants - Apollo (light) and Artemis (dark). Each comes in regular and flat styles, with a carefully curated pastel color palette that's easy on the eyes while maintaining excellent readability. Perfect for long and fun coding sessions.",
  "version": "0.0.7",
  "publisher": "JVLDS",
  "repository": {
    "type": "git",
    "url": "https://github.com/devine-studio/devine-theme.git"
  },
  "engines": {
    "vscode": "^1.9.0"
  },
  "icon": "./icon.png",
  "categories": [
    "Themes"
  ],
  "contributes": {
      "themes": [
        {
        "label": "Devine Apollo",
        "uiTheme": "vs",
        "path": "./themes/apollo-color-theme.json"
      },
      {
        "label": "Devine Apollo Flat",
        "uiTheme": "vs",
        "path": "./themes/apollo-flat-color-theme.json"
      },
      {
        "label": "Devine Artemis",
        "uiTheme": "vs-dark",
        "path": "./themes/artemis-color-theme.json"
      },
      {
        "label": "Devine Artemis Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/artemis-flat-color-theme.json"
      }
    ]
  }
}
